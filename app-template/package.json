    "react-tap-event-plugin": "0.2.1",
    "react-transform-catch-errors": "1.0.1",
    "react-transform-hmr": "1.0.1",
    "redbox-react": "1.2.0",
    "redux-devtools": "^3.0.1",
    "redux-devtools-log-monitor": "^1.0.2",
    "rimraf": "2.5.0",
    "style-loader": "0.13.0",
    "url-loader": "0.5.7",
    "watch": "0.17.1",
    "webpack": "1.12.11",
    "webpack-dev-middleware": "1.4.0",
