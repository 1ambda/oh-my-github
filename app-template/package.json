{
  "name": "yo-omg-basic-app-template",
  "version": "0.0.1",
  "description": "",
  "scripts": {
    "prestart": "npm run remove-dist",
    "start": "parallelshell \"npm run test:watch\" \"npm run open:src\"",
    "open:src": "node tools/srcServer.js",
    "open:dist": "node tools/distServer.js",
    "lint": "eslint src",
    "lint:watch": "watch 'npm run lint' src",
    "clean-dist": "npm run remove-dist && mkdir dist",
    "remove-dist": "node_modules/.bin/rimraf ./dist",
    "build:html": "node tools/buildHtml.js",
    "prebuild": "npm run clean-dist && npm run build:html",
    "build": "npm run test && node tools/build.js && npm run open:dist",
    "test": "cross-env NODE_ENV=test mocha --reporter progress --compilers js:babel/register --recursive \"./src/**/*.spec.js\"",
    "test:watch": "npm run test -- --watch"
  },
  "author": "1amb4a@gmail.com",
  "license": "Apache-2.0",
  "dependencies": {
    "babel-polyfill": "^6.3.14",
    "isomorphic-fetch": "^2.2.1",
    "material-ui": "^0.14.4",
    "moment": "^2.11.2",
    "object-assign": "4.0.1",
    "randomcolor": "^0.4.3",
    "react": "^0.14.7",
    "react-addons-create-fragment": "^0.14.7",
    "react-dom": "^0.14.7",
    "react-paginate": "^0.5.4",
    "react-redux": "4.0.6",
    "react-tooltip": "^1.0.4",
    "redux": "3.0.5",
    "redux-actions": "^0.9.1",
    "redux-logger": "^2.4.0",
    "redux-promise": "^0.5.1",
    "redux-thunk": "^1.0.3",
    "tcomb-react": "^0.8.13"
  },
  "devDependencies": {
    "autoprefixer": "^6.3.1",
    "babel": "5.8.23",
    "babel-eslint": "4.1.7",
    "babel-loader": "5.1.4",
    "babel-plugin-react-display-name": "2.0.0",
    "babel-plugin-react-transform": "1.1.1",
    "browser-sync": "2.11.1",
    "chai": "3.4.1",
    "cheerio": "0.19.0",
    "colors": "1.1.2",
    "cross-env": "1.0.7",
    "css-loader": "0.23.1",
    "eslint": "1.10.3",
